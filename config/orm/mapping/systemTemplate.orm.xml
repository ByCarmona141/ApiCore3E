<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Entity\systemTemplate" table="systemTemplate">
        <id name="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="name" length="32" nullable="false" column="`name`"/>
        <field name="json" type="text" nullable="true" column="`json`"/>
        <field name="header" type="text" nullable="true" column="`header`"/>
        <field name="body" type="text" nullable="false" column="`body`"/>
        <field name="footer" type="text" nullable="true" column="`footer`"/>
        <field name="orientation" type="integer" nullable="false" column="`orientation`"/>
        <field name="size" type="integer" nullable="false" column="`size`"/>
        <field name="headerSpacing" type="integer" nullable="true" column="`headerSpacing`"/>
        <field name="footerSpacing" type="integer" nullable="true" column="`footerSpacing`"/>
        <field name="frontPage" type="text" nullable="true" column="`frontPage`"/>
        <field name="marginLeft" type="integer" nullable="true" column="`marginLeft`"/>
        <field name="marginRight" type="integer" nullable="true" column="`marginRight`"/>
        <field name="marginTop" type="integer" nullable="true" column="`marginTop`"/>
        <field name="marginBottom" type="integer" nullable="true" column="`marginBottom`"/>
        <field name="script" type="text" nullable="true" column="`script`"/>
    </entity>
</doctrine-mapping>